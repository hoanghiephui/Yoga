<?xml version="1.0" encoding="utf-8"?>
<com.getbase.floatingactionbutton.FloatingActionsMenu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fab"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="bottom|right"
    android:layout_margin="16dp"
    app:fab_labelStyle="@style/menu_labels_style"
    app:fab_addButtonColorNormal="@color/colorPrimary"
    app:fab_addButtonColorPressed="@color/colorPrimaryDark">

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fab_icon="@mipmap/ic_send_white"
        app:fab_colorNormal="#E91E63"
        app:fab_title="@string/abc_shareactionprovider_share_with" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fab_icon="@mipmap/ic_refresh_white_24dp"
        app:fab_colorNormal="#E91E63"
        app:fab_title="@string/update" />
</com.getbase.floatingactionbutton.FloatingActionsMenu>